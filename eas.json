{
  "cli": {
    "version": ">= 16.0.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "env": {
        "APP_VARIANT": "dev"
      },
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "env": {
        "APP_VARIANT": "dev"
      },
      "distribution": "internal"
    },
    "production": {
      "env": {
        "APP_VARIANT": "prod"
      },
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {}
  }
}
